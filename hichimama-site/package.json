{
  "name": "hichimama-site",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview:css": "tailwindcss -i ./app/globals.css -o ./docs/preview/styles.css --content \"./app/**/*.{js,ts,jsx,tsx}\" \"./components/**/*.{js,ts,jsx,tsx}\" \"./docs/preview/**/*.html\"",
    "preview:open": "npm run preview:css && open ./docs/preview/index.html",
    "start:prod:local": "next build && next start -p 3000 -H 127.0.0.1",
    "dev:lan": "next dev --hostname 0.0.0.0 --port 3000"
  },
  "dependencies": {
    "@portabletext/react": "^3.0.11",
    "@sanity/client": "^6.15.15",
    "groq": "^3.53.0",
    "next": "^14.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "19.1.12",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.2"
  }
}
